<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.serviceModel>
		<bindings>
			<basicHttpBinding>
				<binding name="PropietarioPortBinding" />
				<binding name="VehiculoPortBinding" />
				<binding name="VehiculoPropietarioPortBinding" />
			</basicHttpBinding>
		</bindings>
		<client>

			<!-- PROPIETARIO -->
			<endpoint address="http://localhost:8080/TransitSoftWS/Propietario"
					  binding="basicHttpBinding"
					  bindingConfiguration="PropietarioPortBinding"
					  contract="PropietarioWS.IPropietario"
					  name="PropietarioPort" />

			<!-- VEHICULO -->
			<endpoint address="http://localhost:8080/TransitSoftWS/Vehiculo"
					  binding="basicHttpBinding"
					  bindingConfiguration="VehiculoPortBinding"
					  contract="VehiculoWS.IVehiculo"
					  name="VehiculoPort" />

			<!-- VEHICULO-PROPIETARIO -->
			<endpoint address="http://localhost:8080/TransitSoftWS/VehiculoPropietario"
					  binding="basicHttpBinding"
					  bindingConfiguration="VehiculoPropietarioPortBinding"
					  contract="VehiculoPropietarioWS.IVehiculoPropietario"
					  name="VehiculoPropietarioPort" />

		</client>
	</system.serviceModel>
</configuration>